.search-view{
  position: fixed;
  top: 0;
  padding-top: 29rpx;
  padding-bottom: 30rpx;
  left: 30rpx;
  width:690rpx;
  background-color: #fff;
  z-index: 111;
}
.search-content{
  float: left;
  position: relative;
  width: 620rpx;
  height: 70rpx;
   background-color: #ececec;
   border-radius:10rpx;
}
.search-icon{
  position: absolute;
  top: 13rpx;
  left: 20rpx;
  width: 44rpx;
  height: 44rpx;
}
.search-input{
  width:100%;
  height: 100%;
  font-size:28rpx;
  line-height:70rpx; 
  padding-left:84rpx;
  box-sizing:border-box;
}
.placeholder{
  color: #787878;
  font-size: 30rpx;
}
.search-text{
  float:right;
  letter-spacing:-1rpx;
  text-align:right;
  width: 70rpx;
  line-height:70rpx;
  font-size: 30rpx;
	color: #ea4037;
}
.search-del{
  position: absolute;
  width: 38rpx;
	height: 38rpx;
  top: 16rpx;
  right: 20rpx;
  z-index: 111;
}
.app-main{
  width: 690rpx;
  margin: 0 auto;
  padding-top: 99rpx;
}
.history{
  margin-top: 24rpx;
  display: flex;
  justify-content: space-between;
}
.history-span{
  font-weight: bold;
  font-size: 30rpx;
  color: #545454;
}
.history-clear{
  width: 36rpx;
	height: 34rpx;
  background-image: url(https://aishangnet.club/static/mina_pic/delete.png);
  background-size: cover;
}
.label-view{
  padding-top: 29rpx;
}
.search-label{
  float: left;
  width: 200rpx;
	height: 60rpx;
  line-height: 60rpx;
	background-color: #ececec;
	border-radius: 30rpx;
  text-align: center;
  font-size: 28rpx;
  color: #383838;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  margin: 0 20rpx 30rpx 0;
}
.time_quantum{
  width: 690rpx;
	height: 80rpx;
  margin: 20rpx auto;
  border-radius: 10rpx;
  background-color: #ececec;
  display:flex;
}
.time_item{
  flex: 1;
  height: 100%;
  font-size: 30rpx;
  color: #000;
  line-height: 80rpx;
  text-align: center;
}
.time_item_active{
  color: #ea4037;
}
.sort-img{
  width: 16rpx;
	height: 22rpx;
  margin-left: 10rpx;
}
.item_box{
  height: 220rpx;
  width: 690rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
}

.item_img{
  width:220rpx;
  height: 220rpx;
  border-radius: 10px 0px 0px 10px;
}
.item_info_box{
  height: 100%;
  width:450rpx;
  margin-left: 20rpx;
}
.item_img,.item_info_box{
  float: left;
}
.item_name{
  margin-top:20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
  color: #333333;
}

.item_discount{
  display:inline-block;
  position: relative;
	height: 40rpx;
  margin: 18rpx 0 16rpx 0;
  z-index:1;
}

.quan_img{
  width: 100%;
	height: 40rpx;
  position: absolute;
  z-index: -1;
}
.quan_text{
  padding: 0 14rpx;
  line-height: 40rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
}
.discount_unit{
  font-size: 16rpx;
}
.old_box{
  font-size: 24rpx;
  color: #c3c3c3;
  letter-spacing: 0;
}
.selledline{
  margin: 27rpx;
}
.oldprice{
  text-decoration:line-through
}
.item_sale{
  float: right;
  font-size: 28rpx;
  color: #D4D2D3;
  margin-top: 10rpx;
}
.price_box{
  margin-top:13rpx;
}
.item_price{
  float: left;
  color: #c3c3c3;
  font-size: 24rpx;
  line-height: 50rpx;
}
.item_price_num{
  color: #ff1b51;
  font-size: 36rpx;
}
.item_buy{
  float: right;
  height: 44rpx;
  line-height: 44rpx;
  background: #ea4037;
  border-radius: 22rpx;
  color: #FFFFFF;
  padding: 0 12rpx;
  font-size:24rpx;
  text-align: center;
}
.bottom_img {
  width: 23rpx;
  height: 23rpx;
  vertical-align: middle;
}
.item_but_text {
  padding-left: 5rpx;
}
/*上划加载*/
.comment-loadmore{
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding:20rpx 0;
  /* border-top: 2rpx solid #eeeeee; */
  background: #fff;
}
.comment-loadmore-tips{
  font-size: 30rpx;
  color: #999999;
}
.comment-loading {
  margin: 0 10rpx;
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: Loading 1s steps(12, end) infinite;
          animation: Loading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
@-webkit-keyframes Loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}
@keyframes Loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}
.no-more{
  text-align: center;
  color: #999;
}

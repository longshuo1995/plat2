/* pages/my/partner.wxss */
.search_form{
  height: 100rpx;
  width:100%;
  margin:0 auto;
  background: #FED333;
}
.search_input{
  width: 70%;
  height: 80rpx;
  float: left;
  margin-left:10%;
  margin-top:10rpx;
  background: white;
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  padding-left:20rpx;
}
.search_btn{
  width:80rpx;
  height: 80rpx;
  position: relative;
  float:left;
  background: white;
  margin-top:10rpx;
  border-bottom-right-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
.btn_line{
  position: absolute;
  width:4rpx;
  height:60rpx;
  margin-top:10rpx;
  background: #C3C3C3;
}
.search_img{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 50%;
  transform: translate(-50%,50%)
}
.status-box{
    width:100%;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #fff;
}
.status-box .status-label{
    width: 150rpx;
    height: 100%;
    text-align: center;
    font-size:28rpx;
    color:#353535;
    box-sizing: border-box;
    position: relative;
}
.status-box .status-label.active{
    color:#e64340;
    border-bottom: 6rpx solid #e64340;
}
.status-box .status-label .red-dot{
    width: 16rpx;
    height: 16rpx;
    position: absolute;
    left: 116rpx;
    top:23rpx;
    background-color: #f43530;
    border-radius: 50%;
}

/* user——list */
/* pages/hot_sell/hot_sell.wxss */
.clearfix:after{
content:"";
display:block;
visibility:hidden;
clear:both;
height:0;
}
.clearfix{ /* 为了照顾ie6浏览器*/
zoom:1;
}
.item_box{
  height: 240rpx;
  border-bottom: 1px solid #EFEFED;
  position: relative;
}
.item_index{
  width:120rpx;
  height: 100%;
  position: relative;
}
.item_index_number{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  transform: translate(-50%,-50%); 
  left: 50%;
  top: 50%;
  line-height: 60rpx;
  text-align: center;
  background: #B3B1B2;
  border-radius: 30rpx;
  color: white;
  box-shadow: 2px 2px 5px #B3B1B2;
}
.item_first{
  background: #FFC806;
  box-shadow: 2px 2px 5px #E4A53F;
}
.item_second{
  background: #BAC1CF;
  box-shadow: 2px 2px 5px #9E9BB1;
}
.item_three{
  background: #E2B57C;
  box-shadow: 2px 2px 5px #C78D4A;
}
.item_img{
  width:140rpx;
  height:140rpx;
  margin-top:50rpx;
  margin-left:50rpx;
  border-radius:100rpx;
}
.item_info_box{
  height: 100%;
  min-width: 200rpx;
  max-width:400rpx;
  margin-left:20rpx;
  display: flex;
  flex-flow: column;
  justify-content: center;
}
.item_img,.item_index,.item_info_box{
  float: left;
}
.item_name{
  margin-top:20rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  font-size: 32rpx;
}
.sell_box{
  position: relative;
}
.quan_img{
  width: 100%;
  height: 60rpx;
  position: absolute;
  z-index: -2;
}
.item_discount{
  float: left;
  font-size: 24rpx;
  line-height: 60rpx;
  color: white;
  position: relative;
  letter-spacing:0rpx;
}
.quan_text{
  margin:0 20rpx;
}
.item_sale{
  float: right;
  font-size: 32rpx;
  color: #D4D2D3;
  margin-top: 10rpx;
}
.price_box{
  margin-top:20rpx;
}
.item_price{
  float: left;
  color: #CA3954;
  font-size: 36rpx;
}
.item_buy{
  float: right;
  background: #E85654;
  border-radius: 25rpx;
  color: #FFFFFF;
  padding: 5rpx 20rpx;
  font-size:30rpx;
}

/*上划加载*/
.comment-loadmore{
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding:20rpx 0;
  /* border-top: 2rpx solid #eeeeee; */
  background: #fff;
}
.comment-loadmore-tips{
  font-size: 30rpx;
  color: #999999;
}
.comment-loading {
  margin: 0 10rpx;
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: Loading 1s steps(12, end) infinite;
          animation: Loading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
@-webkit-keyframes Loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}
@keyframes Loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}
.no-more{
  text-align: center;
  color: #999;
}



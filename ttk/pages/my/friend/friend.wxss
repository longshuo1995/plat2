page {
    min-height: 100%;
    background-color: #efeff4;
    height: 100%;
}

.weui-tab__content::before {
    display: none;
}

.weui-tab__content::after {
    display: none;
}

.weui-cells::after {
    display: none;
}

.weui-cells::before {
    display: none;
}

.weui-cells {
    line-height: 32rpx;
}

.weui-cell__hd image {
    margin-right: 22rpx;
    vertical-align: middle;
    width: 110rpx;
    height: 110rpx;
}

.head_pic {
    border-radius: 50%;
}

.weui-cell__ft {
    font-size: 30rpx;
    color: #4e4e4e;
}

.weui-cell__ft {
    color: #4e4e4e;
}

.scrolltip {
    text-align: center;
    padding: 16rpx 0;
    font-size: 28rpx;
    color: #9b9b9b;
    background-color: #efeff4;
}

.null-data {
    text-align: center;
    padding-top: 120rpx;
    font-size: 28rpx;
    color: #4a4a4a;
    background-color: #efeff4;
    border: none;
}

.xiajicel {
    display: flex;
    flex-direction: column;
    align-content: space-around;
}

.weui-cell {
    padding: 20rpx 30rpx 20rpx 21rpx;
}

.shouji {
    color: #9b9b9b;
    font-size: 12px;
    margin-top: 12rpx;
}

.weui-navbar:first-child::after {
    content: " ";
    position: absolute;
    top: 22rpx;
    right: 50%;
    width: 1rpx;
    height: 58rpx;
    background: #efefef;
    z-index: 2;
}

.weui-navbar__item {
    color: #9b9b9b;
    background: #fff;
    padding: 34rpx 0;
}

.weui-navbar__item.weui-bar__item_on {
    color: #252525;
}

.weui-navbar__title {
    font-size: 24rpx;
}

.weui-navbar__slider {
    background-color: #000;
    width: 114rpx;
}

.sreach_icon {
    width: 36rpx;
    height: 36rpx;
    position: absolute;
    margin-left: 60rpx;
    font-size: 18rpx;
    margin-top: 20rpx;
}

.sreach_icon_del {
    width: 28rpx;
    height: 28rpx;
    margin-top: 16rpx;
}

.sreach_button {
    background: none!important;
    color: #000!important;
    width: 130rpx;
    margin-top: -10rpx;
    font-size: 32rpx;
}

.sreach_input_bk {
    display: flex;
    flex-direction: row;
    background-color: #f0f0f0;
    border-radius: 100rpx;
    margin: 10rpx 16rpx 10rpx 36rpx;
    width: 75vw;
    height: 54rpx;
}

.sreach_input {
    font-size: 28rpx;
    padding: 0rpx 32rpx 8rpx 72rpx;
    height: 54rpx;
    line-height: 54rpx;
    width: 50vw;
}

.icon {
    width: 50rpx;
    height: 50rpx;
}

.leader_img {
    width: 106rpx;
    height: 106rpx;
    border-radius: 50%;
}

.leader {
    padding-left: 30rpx;
    display: flex;
    flex-direction: row;
    margin-top: 20rpx;
}

.leader_text {
    font-size: 30rpx;
    margin-left: 20rpx;
}

.recommend {
    background-color: #f9e276;
    border-radius: 30rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
    font-size: 24rpx;
    color: #444b54;
}

.cofrimDialog {
    width: 60vw;
    height: 270rpx;
    margin-left: 16vw;
    background-color: #fff;
    text-align: center;
    padding-top: 60rpx;
    position: fixed;
    top: 30%;
    z-index: 999;
    padding-left: 30rpx;
    padding-right: 30rpx;
    border-radius: 10rpx;
    padding-bottom: 20rpx;
}

.btn_cofrim {
    width: 232rpx;
    height: 68rpx;
    font-size: 24rpx;
    background: none;
    background-color: #f9e276;
    border-radius: 40rpx;
    line-height: 68rpx;
}

.btn_cofrim::after {
    border: none;
}

.bk_dialog {
    width: 100vw;
    height: 100vh;
    background-color: #000;
    opacity: 0.5;
    position: fixed;
    z-index: 998;
    top: 0rpx;
    left: 0rpx;
}

.leader_rights {
    width: 150rpx;
    height: 40rpx;
    background-color: #f9e276;
    font-size: 26rpx;
    border-radius: 40rpx;
    text-align: center;
    margin-left: 20rpx;
}

.hehuoren_icon {
    width: 56rpx;
    height: 56rpx;
    margin-top: 60rpx;
    margin-left: -40rpx;
}